# Caddyfile

netsumap.baughcl.com {
    
    handle /* {
		reverse_proxy quart_app:5000
	}

    handle /enroll* {
		reverse_proxy quart_app:5000
	}

	handle /unenroll* {
		reverse_proxy quart_app:5000
	}

    handle /json_inference* {
		reverse_proxy quart_app:5000
	}

	handle /pong* {
		reverse_proxy quart_app:5000
	}

	handle /iperf* {
        reverse_proxy iperf_server:6363
    }

}


