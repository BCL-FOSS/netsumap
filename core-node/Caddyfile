    handle /* {
		reverse_proxy quart_app:5000
	}

    handle_path /register/* {
		reverse_proxy quart_app:5000
	}

    handle_path /csv_inference/* {
		reverse_proxy quart_app:5000
	}

    handle_path /json_inference/* {
		reverse_proxy quart_app:5000
	}

    handle_path /rmprobe/* {
		reverse_proxy quart_app:5000
	}

    handle_path /netmetadata/* {
		reverse_proxy quart_app:5000
	}

    handle_path /isup/* {
		reverse_proxy quart_app:5000
	}

    handle_path /allprobes/* {
		reverse_proxy quart_app:5000
	}

}


